<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.codechilli.interruption-alerts</string>
    <key>ProgramArguments</key>
<array>
    <string>/opt/homebrew/bin/php</string>
    <string>/Users/codechilli/Scripts/interruption-alerts/notify-interruption-alerts.php</string>
</array>
    <key>StartInterval</key>
    <integer>1800</integer>
    <key>RunAtLoad</key>
    <true/>
    <key>StandardOutPath</key>
    <string>/Users/codechilli/Scripts/interruption-alerts/interruption-alerts.log</string>
    <key>StandardErrorPath</key>
    <string>/Users/codechilli/Scripts/interruption-alerts/interruption-alerts-error.log</string>
    <key>EnvironmentVariables</key>
    <dict>
        <key>PATH</key>
        <string>/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin</string>
    </dict>
</dict>
</plist>